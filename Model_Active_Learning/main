from active_learning import Active_learning
import pandas as pd

def main():
    labels = set(('stairs_up', 'stairs_down', 'walking', 'running'))
    al = Active_learning(r'running_data/features_Walking_scaled.csv', labels)
    al.training(150)
    print(f'number of errors: {al.testing(30)}')
    al.plotting()

if __name__ == '__main__':
    main()